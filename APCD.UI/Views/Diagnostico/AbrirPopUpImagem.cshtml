@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_LayoutPopUp.cshtml";
}
<script type="text/javascript" language="javascript">
    $(window).load(
        function () {
            var Variavel = location.search.split("?");
            var caminho = Variavel[1].split("=");
            $("#ImgPopUp").attr({ src: caminho[1] });
        }
    );
</script>
<center>
    <div style="text-align: center; width: 75%; height: 75%;">
        <img id="ImgPopUp" alt="Imagem" src="" style="width: 100%; height: 100%; text-align: center" />
    </div>
</center>
