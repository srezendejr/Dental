@{
    ViewBag.Title = "Novo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@*<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>*@
@using (Html.BeginForm("Salvar", "Procedimento"))
{
    <fieldset>
        <legend>Procedimentos</legend>
        @Html.ValidationSummary(true)
        <table id="TableForms">
            <tr>
                <td>
                    @Html.Partial("_FormProcedimento")
                </td>
            </tr>
            <tr>
                <th colspan="3" style="background-color: transparent !Important;">
                    <span>
                        <input type="button" value="Voltar" id="btnVoltar" onclick="javascript:window.location='@Url.Content("~/Procedimento/Index")';" />
                    </span><span>
                        <input type="submit" value="Salvar" id="btnSalvar" />
                    </span>
                </th>
            </tr>
        </table>
    </fieldset>
}